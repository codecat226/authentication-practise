<%- include("./layouts/header.ejs") %>
<h1>Log in</h1>
<form action="/login" method="post">
  <input
    type="email"
    name="email"
    placeholder="email address"
    required
  />
  <input
    type="password"
    name="password"
    placeholder="password"
    required
  />
  <button type="submit">Login</button>
</form>